{
  "dashboard": {
    "title": "API Performance - Gest√£o de Notas",
    "panels": [
      {
        "id": 1,
        "title": "Requests por Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (route) (rate(django_route_requests_total[5m]))",
            "legendFormat": "{{route}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Tempo de Resposta por Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(django_route_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}} - 95th"
          }
        ]
      },
      {
        "id": 3,
        "title": "Erros HTTP",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (status) (rate(django_route_requests_total{status=~\"4..|5..\"}[5m]))",
            "legendFormat": "HTTP {{status}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Upload de Arquivos",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(django_route_requests_total{route=\"processar-nota\", method=\"POST\"}[1h])) * 3600",
            "legendFormat": "Uploads/hora"
          }
        ]
      }
    ]
  }
}